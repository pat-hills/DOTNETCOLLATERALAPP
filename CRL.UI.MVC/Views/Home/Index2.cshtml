@{
    Layout =null;
}
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title></title>
<meta name="keywords" content="" />
<meta name="description" content="" />
<script  type="text/javascript" src="~/Content/HomePage/jquery-1.7.1.min.js"></script>
 <script type="text/javascript" src="~/Content/HomePage/jquery.slidertron-1.3.js"></script>
<link href="http://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700,900" rel="stylesheet" />
<link href="~/Content/HomePage/default.css" rel="stylesheet" type="text/css" media="all"  />
<link href="~/Content/HomePage/fonts.css" rel="stylesheet" />
<!--[if IE 6]><link href="default_ie6.css" rel="stylesheet" type="text/css" /><![endif]-->

</head>
<body>
<div id="header-wrapper">
	<div id="header" class="container">
                    <img class="logoimg" src="~/Content/HomePage/Images/cblLogo.gif" alt="CBL Logo"/>
		<div  id="logo">

			<h1><a href="#">Collateral Registry System</a></h1>		
		</div>
		<div id="menu">
			<ul>
				<li class="current_page_item"><a href="#" accesskey="1" title="">Homepage</a></li>
				<li>@Html.ActionLink("Register", "SelectRegistrationType", "MembershipRegistration", new { Area = "Membership" }, new  { })</li>
				<li><a href="#" accesskey="3" title="">Search</a></li>
				<li><a href="#" accesskey="4" title="">Help</a></li>
				<li><a href="#" accesskey="5" title="">Contact Us</a></li>
			</ul>
		</div>
	</div>
</div>
<div id="header-featured"> </div>
<div id="wrapper">
	<div id="featured-wrapper">
		<div id="featured" class="container">
			<div class="column1"> <span class="icon icon-key"></span>
				<div class="title">
					<h2>@Html.ActionLink("Client Login", "Dashboard", "Home",null, new { style = "text-decoration:false" })</h2>
				</div>
				<p>If you already have an account in the system click on the icon above to login tinto the Collateral Registry System.</p>
			</div>
			<div class="column2"> <span class="icon icon-legal"></span>
				<div class="title">
					<h2>@Html.ActionLink("Register", "SelectRegistrationType", "MembershipRegistration", new { Area = "Membership" }, new  { })</h2>
				</div>
				<p>If you wish to register as a user of the Registry and would like to apply for a Client Account, click on the icon above</p>
			</div>
			<div class="column3"> <span class="icon icon-unlock"></span>
				<div class="title">
					<h2>Search</h2>
				</div>
				<p>If you wish to perform a public search then click on the link above</p>
			</div>
			<div class="column4"> <span class="icon icon-wrench"></span>
				<div class="title">
					<h2>Help</h2>
				</div>
				<p>For more information concerning varous activites in the Collateral Registry click the link above.</p>
			</div>
		</div>
		<div id="extra" class="container">
			<h2>Technical Assistance</h2>
			<span>For technical assistance please contact the Collateral Registry technical team of Liberia</span> </div>
		

	</div>
</div>
<div id="copyright" class="container">
	<p><a>Copyright (c) 2013 Collateral Registry Liberia All rights reserved.</a>.</p>
</div>
</body>
</html>
